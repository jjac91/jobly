{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "required": [
        "title",
        "companyHandle"
    ],
    "additionalProperties": false,
    "properties": {
        "title": {
            "type": "string",
            "minLength": 1
        },
        "salary": {
            "type": "integer",
            "minimum": 0
        },
        "equity": {
            "type": "number",
            "minimum": 0,
            "maximum": 1.0
        },
        "companyHandle": {
            "type": "string",
            "maxLength": 25,
            "minimum": 1
        }
    }
}